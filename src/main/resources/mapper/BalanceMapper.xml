<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="fingertips.backend.home.mapper.BalanceMapper">

    <select id="getBalance" parameterType="String" resultType="BalanceDTO">
        SELECT SUM(balance) as balance
        FROM account_book
        WHERE member_id = #{memberId}
    </select>



</mapper>
